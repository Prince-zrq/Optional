#include<stdio.h>
int main(){
	int a1,a2;
	char op;
	scanf("%d %c %d",&a1,&op,&a2);
	if(a2!=0)
	switch(op){
		case'+':printf("%d",a1+a2);break;
		case'-':printf("%d",a1-a2);break;
		case'*':printf("%d",a1*a2);break;
		case'/':printf("%d",a1/a2);break;
		case'%':printf("%d",a1%a2);break;
		default:printf("ERROR");break;
	}
}
